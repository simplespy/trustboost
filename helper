{
    keyring="--keyring-backend test --keyring-dir ./data/ibc-$1"
    chain="ibc-$1"
    node=http://127.0.0.1:2655$1
    user="user"

    set -e
    EXEC_MSG='{ "delete_all_records": { "name": "test_from_trustboost_sept"} }'
    set -x
